{
	"compilerOptions": {
		"allowSyntheticDefaultImports": false,
		"baseUrl": "./",
		"paths": {
			"@package.json": [
				"package.json"
			],
			"@public/": [
				"public/"
			],
			"@data/": [
				"src/data/"
			],
			"@config/": [
				"src/config/"
			],
			"@config/*": [
				"src/config/*"
			],
			"@pages/": [
				"src/Pages/"
			],
			"@pages/*": [
				"src/Pages/*"
			],
			"@template/": [
				"src/template/"
			],
			"@template/*": [
				"src/template/*"
			],
			"@hoc/": [
				"src/hoc/"
			],
			"@hoc/*": [
				"src/hoc/*"
			],
			"@hooks/": [
				"src/hooks/"
			],
			"@hooks/*": [
				"src/hooks/*"
			],
			"@services/": [
				"src/services/"
			],
			"@services/*": [
				"src/services/*"
			],
			"@store/": [
				"src/store/"
			],
			"@store/*": [
				"src/store/*"
			],
			"@js/": [
				"src/js/"
			],
			"@js/*": [
				"src/js/*"
			],
			"@components/": [
				"src/components/"
			],
			"@components/*": [
				"src/components/*"
			]
		}
	}
}